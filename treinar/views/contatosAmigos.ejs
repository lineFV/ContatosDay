        
        <!DOCTYPE html>
<html lang="en">
    <%- include('head') %>
    <body>
        <%- include('header') %>

        <form method="POST">
            <input placeholder="Nome" name="nome">
            <input placeholder="Email" name="email">
            <input placeholder="Telefone" name="telefone">

            <button>Enviar</button>
        </form>
        <% for(let item of lista){ %>
            <h2>Contato salvo.</h2>
            <h4><%=item.email%></h4>
            <p><%=item.nome%></p>
            <p><%=item.telefone%></p>
        <% } %>
    </body>
</html>